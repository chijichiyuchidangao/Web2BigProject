		function getTime(){
			var date = new Date();
			var hour = date.getHours();
			var minute = date.getMinutes();
			var second = date.getSeconds();
			var hour1 = hour<10?'0' + hour :hour;
			var minute1 = minute<10?'0' + minute :minute;
			var second1 = second<10?'0' + second :second;
			paragraph.innerHTML = hour1 + "：" + minute1 + "："+ second1;
		}
		getTime();
		setInterval(getTime,1000);
		function scroll(){
			var date1 = new Date();
			var year = date1.getFullYear();
			var tis = 0
			if(year == 2019){
				marquee.innerHTML = "距 离 2020 年 仅 剩 " + (day-now.getDate()) + " 天 ! ! ! ";
			}
			else{
				marquee.innerHTML = "今 天 也 是 活 力 满 满 的 一 天 ！ 加 油 ！ 奥 利 给 ！";
			}
			tis++
		}
		scroll();
		// 计算机 by 崔宫健
		var lbt = document.getElementById("lbt");
 		var oNavlist = document.getElementById('navc').children;
 		var out = document.getElementById('out');
 		var navc = document.getElementById("navc");
 		var leftc = document.getElementById("leftc");
 		var rightc = document.getElementById("rightc");
 		var buttenlef = document.getElementById("cbtitle").children;
 		var cblbt = document.getElementById("cblbt");
 		var cblbtr = document.getElementById("cblbtr");
 		var buttenr = document.getElementById("cbtitler").children;
 		var i =1;
 		var timer;
 		var isMoving = false;

 		function next(){
 			i++;
 			navChange();
		    animate(out,{left:-1200*i},function(){
				if(i===6){
					out.style.left = "-1200px";
					i = 1;
				}
			});
 		}
 		function pre(){
 			i--;
 			navChange();
			 animate(out,{left:-1200*i},function(){
				if(i===0){
					out.style.left = "-6000px";
					i = 5;
				}
			});
 		}

 		lbt.onmouseover = function(){
 			animate(leftc,{opacity:50})
 			animate(rightc,{opacity:50})
 			clearInterval(timer)
 		}
 		lbt.onmouseout = function(){
 			animate(leftc,{opacity:0})
 			animate(rightc,{opacity:0})
 			timer = setInterval(next,2000)
 		}
 		leftc.onclick = next;
 		rightc.onclick = pre;

 		//按钮
 		for(var j = 0;j<oNavlist.length;j++){
 			oNavlist[j].idx = j;
 			oNavlist[j].onclick = function(){
 				i = this.idx+1;
 				navChange();
 				animate(out,{left:-1200*i});
 			}
 		}
 		//变红
 		function navChange(){
 			for(var j = 0;j<oNavlist.length;j++){
 				oNavlist[j].className = ' ';
 			}
 			if(i === 6){
 				oNavlist[0].className = 'active';
 			}else if(i===0){
 				oNavlist[4].className = 'active';
 			}else{
 				oNavlist[i-1].className = 'active';
 			}
 		}
 		//帖子切换
 		console.log(buttenlef[2]);
 		buttenlef[3].onclick = function(){
 			cblbt.style.left = "0";
 		};
 		buttenlef[2].onclick = function(){
 			cblbt.style.left = "-596px";
 		};
